Grammar

    0 $accept: unit "end of file"

    1 unit: assignments exp

    2 assignments: %empty
    3            | assignments assignment

    4 assignment: "identifier" ":=" exp

    5 exp: exp "+" exp
    6    | exp "-" exp
    7    | exp "*" exp
    8    | exp "/" exp
    9    | "(" exp ")"
   10    | "identifier"
   11    | "number"


